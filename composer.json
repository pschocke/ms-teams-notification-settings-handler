{
  "name": "pschocke/ms-teams-notification-settings-handler",
  "description": "Send Microsoft Teams notifications via the laravel notificationSettings package",
  "keywords": [
    "Laravel",
    "Laravel Notifications",
    "MS teams"
  ],
  "homepage": "https://github.com/pschocke/ms-teams-notification-settings-handler",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Schocke",
      "email": "patrick@ausbildung-ms.de",
      "homepage": "https://github.com/pschocke",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "~6|~7|~8|~9|~10",
    "laravel-notification-channels/microsoft-teams": "^1.2",
    "pschocke/laravel-notification-settings": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "orchestra/testbench": "5.*|6.*",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "pschocke\\MSTeamsNotificationSettings\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pschocke\\MSTeamsNotificationSettings\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {}
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
